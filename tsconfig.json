{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@drinkweise/*": [
        "./src/*"
      ],
      "@drinkweise/ui/*": [
        "./src/components/ui/*"
      ],
      "@env": ["./src/lib/environment.js"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}