{
  "name": "drinkweise",
  "version": "0.1.0",
  "main": "expo-router/entry",
  "scripts": {
    "install:immutable": "yarn install --immutable",
    "start": "expo start --dev-client",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "prebuild": "expo prebuild",
    "build:production:ios": "eas build --profile production --platform ios",
    "build:production:android": "eas build --profile production --platform android",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "audit:ci": "yarn audit --level high",
    "lint:write": "npx eslint --fix .",
    "lint:check": "npx eslint .",
    "lint:ci": "npx eslint .",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "format:ci": "npx prettier --check .",
    "type:check": "tsc --noEmit",
    "web": "expo start --web",
    "supabase:gen:types": "npx supabase gen types --lang typescript --local > src/lib/types/generated/supabase.types.ts",
    "supabase:gen:migrate": "sh -c 'MIGRATION=$(npx supabase migration new \"$0\") && echo \"$MIGRATION\" && npx supabase db diff > $(echo \"$MIGRATION\" | grep -o \"supabase/migrations/.*\")'",
    "app:release": "np --no-publish --no-cleanup --no-release-draft --no-tests",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "@dev-plugins/react-native-mmkv": "~0.2.0",
    "@dev-plugins/react-navigation": "~0.2.0",
    "@dev-plugins/react-query": "~0.2.0",
    "@expo/react-native-action-sheet": "^4.1.0",
    "@expo/vector-icons": "^14.0.0",
    "@gorhom/bottom-sheet": "^5.0.6",
    "@hookform/resolvers": "^4.1.3",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-community/slider": "4.5.5",
    "@react-native-google-signin/google-signin": "^13.2.0",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/native": "^7.0.3",
    "@reduxjs/toolkit": "^2.6.1",
    "@rn-primitives/avatar": "^1.1.0",
    "@rn-primitives/slot": "^1.1.0",
    "@roninoss/icons": "^0.0.4",
    "@shopify/flash-list": "1.7.3",
    "@shopify/react-native-skia": "1.5.0",
    "@supabase/supabase-js": "^2.38.4",
    "@tanstack/query-sync-storage-persister": "^5.74.6",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/react-query-persist-client": "^5.74.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "expo": "~52.0.47",
    "expo-apple-authentication": "~7.1.3",
    "expo-blur": "~14.0.3",
    "expo-camera": "~16.0.18",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-device": "~7.0.3",
    "expo-haptics": "~14.0.1",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-localization": "~16.0.1",
    "expo-navigation-bar": "~4.0.9",
    "expo-router": "~4.0.21",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-store-review": "~8.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "~14.0.2",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-gifted-charts": "^1.4.63",
    "react-native-keyboard-controller": "^1.17.5",
    "react-native-mmkv": "^3.2.0",
    "react-native-pager-view": "6.5.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-uitextview": "^1.1.4",
    "react-native-web": "~0.19.10",
    "react-redux": "^9.2.0",
    "redux-devtools-expo-dev-plugin": "^1.0.0",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@tanstack/eslint-plugin-query": "^5.73.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-testing-library": "^7.1.1",
    "jest": "~29.7.0",
    "jest-expo": "~52.0.6",
    "np": "^10.2.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)"
    ]
  }
}
